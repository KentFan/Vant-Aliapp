<view class='page-container-classname'>
                <van-tabs active='{{ active }}' color='#1989fa' onChildRef='saveChildRef1' onChange='onSwitch'>
  <van-tab title='基础图标' custom-class='demo-tab-pane' onChildRef='saveChildRef2'>
    <van-col a:for='{{ icons.basic }}' a:key='*this' custom-class='col' span='{{6}}' ref-numbers='{{ icons.basic }}' onChildRef='saveChildRef3'>
      <van-icon name='{{ item }}' size='32px' custom-class='icon' onChildRef='saveChildRef4'>
      </van-icon>
      <view class='text'>
        {{ item }}
      </view>
    </van-col>
  </van-tab>
  <van-tab title='线框风格' custom-class='demo-tab-pane' onChildRef='saveChildRef5'>
    <van-col a:for='{{ icons.outline }}' a:key='*this' custom-class='col' span='{{6}}' ref-numbers='{{ icons.outline }}' onChildRef='saveChildRef6'>
      <van-icon name='{{ item }}' size='32px' custom-class='icon' onChildRef='saveChildRef7'>
      </van-icon>
      <view class='text'>
        {{ item }}
      </view>
    </van-col>
  </van-tab>
  <van-tab title='实底风格' custom-class='demo-tab-pane' onChildRef='saveChildRef8'>
    <van-col a:for='{{ icons.filled }}' a:key='*this' custom-class='col' span='{{6}}' ref-numbers='{{ icons.filled }}' onChildRef='saveChildRef9'>
      <van-icon name='{{ item }}' size='32px' custom-class='icon' onChildRef='saveChildRef10'>
      </van-icon>
      <view class='text'>
        {{ item }}
      </view>
    </van-col>
  </van-tab>
</van-tabs>
            </view>