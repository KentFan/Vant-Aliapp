<view class='custom-tab-bar-index {{className}}' onChildRef='saveChildRef0'>
  <van-tabbar active='{{ active }}' onChange='onChange' onChildRef='saveChildRef1'>
    <van-tabbar-item a:for='{{ list }}' a:key='*this' icon='{{ item.icon }}' ref-numbers='{{ list }}' onChildRef='saveChildRef2'>
      {{
    item.text
  }}
    </van-tabbar-item>
  </van-tabbar>
</view>