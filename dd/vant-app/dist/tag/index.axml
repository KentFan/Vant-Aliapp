  </import-sjs>
  <view is-inline='true' class="{{customClass}} {{myClass}} {{ utils.bem('tag', [size, { mark, plain, round }]) }} {{ plain ? 'van-hairline--surround' : '' }}" style='{{ style }}' _parent_ref="{{isMounted}}">
    <slot>
    </slot>
  </view>
</view>