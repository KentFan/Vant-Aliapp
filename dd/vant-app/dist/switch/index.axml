<view class='switch-index {{className}}' onChildRef='saveChildRef0' _parent_ref="{{isMounted}}">
  <import-sjs _parent_ref="{{isMounted}}" from='../wxs/utils.sjs' name='utils'>
  </import-sjs>
  <view class="{{customClass}} {{ utils.bem('switch', { on: value === activeValue, disabled }) }}" style="font-size: {{ size }}; {{ (checked ? activeColor : inactiveColor) ? 'background-color: ' + (checked ? activeColor : inactiveColor ) : '' }}" _parent_ref="{{isMounted}}" onTap='onClick'>
    <view class='van-switch__node {{nodeClass}}' _parent_ref="{{isMounted}}">
      <van-loading a:if='{{ loading }}' size='50%' custom-class='van-switch__loading' _parent_ref="{{isMounted}}" onChildRef='saveChildRef1'>
      </van-loading>
    </view>
  </view>
</view>