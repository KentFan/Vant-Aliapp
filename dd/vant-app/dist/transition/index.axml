<view class='transition-index {{className}}' onChildRef='saveChildRef0' _parent_ref="{{isMounted}}">
  <view a:if='{{ inited }}' class='van-transition {{customClass}} {{ classes }}' style="-webkit-transition-duration:{{ currentDuration }}ms; transition-duration:{{ currentDuration }}ms; {{ display ? '' : 'display: none;' }} {{ customStyle }}" onTap='onTap' _parent_ref="{{isMounted}}" onTransitionend='onTransitionEnd'>
    <slot>
    </slot>
  </view>
</view>