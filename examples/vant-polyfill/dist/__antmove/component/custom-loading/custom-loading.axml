<view class='{{className}}' ref='saveChildRef0'>
  <view class='Loading_mask' a:if='{{!hidden}}'>
    <view class='Loading_box'>
      <view class='icon'>
        <view class='loading_icon'>
          <image class='loading_img' src='../component/custom-loading/Loading.png' mode='aspectFill'>
          </image>
        </view>
      </view>
      <view a:if='{{textContent}}'>
        {{textContent}}
      </view>
      <slot>
      </slot>
    </view>
  </view>
</view>