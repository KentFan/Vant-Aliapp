{
  "name": "vant-aliapp",
  "version": "1.0.13",
  "description": "vant alipay mini app components",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "antmove wx-alipay -i ./wx/vant-app -o ./alipay/vant-app",
    "build-dd": "antmove wx-alipay -i ./wx/vant-app -o ./dd/vant-app",
    "build-npm": "rm -rf dist && cp -R alipay/vant-app dist && babel alipay/vant-app -d dist && rm -rf dist/node_modules dist/.tea",
    "pub": "npm run build && npm publish"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "amap-appx@service.autonavi.com",
  "license": "GPL-3.0"
}
